    <owl:Class rdf:about="https://contsys.org/concept/clinical_process_outcome_evaluation">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/healthcare_evaluation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#evaluatesOutcomeOf"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/clinical_process"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">healthcare evaluation where the effects of a clinical process on a health state are assessed against the target condition and/or a health condition representing the input health state</rdfs:comment>
        <skos:note xml:lang="en">NOTE 1 The subject of care and healthcare professionals are the main contributors to a clinical process outcome evaluation.

NOTE 2 The target condition represents a requirement for the clinical process.</skos:note>
    </owl:Class>
    


    <!-- https://contsys.org/concept/clinical_report -->

    <owl:Class rdf:about="https://contsys.org/concept/clinical_report">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/health_record_extract"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#isReceivedAs"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/non-ratified_healthcare_information"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">health record extract conveying specifically focused healthcare information in order to fulfil current information needs of the recipient</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/consent_competence -->

    <owl:Class rdf:about="https://contsys.org/concept/consent_competence">
        <rdfs:subClassOf rdf:resource="http://ontology.ip.rm.cnr.it/ontologies/DOLCE-Lite#Mental-Object"/>
        <rdfs:comment xml:lang="en">capability of the subject of care and/or the subject of care proxy to give informed consent or dissent</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/considered_condition -->

    <owl:Class rdf:about="https://contsys.org/concept/considered_condition">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/potential_health_condition"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#isBasedOn"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/observed_condition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">potential health condition considered by a healthcare actor on the basis of one or more observed conditions</rdfs:comment>
        <rdfs:seeAlso xml:lang="en">considered health condition</rdfs:seeAlso>
    </owl:Class>
    


    <!-- https://contsys.org/concept/contact -->

    <owl:Class rdf:about="https://contsys.org/concept/contact">
        <rdfs:subClassOf rdf:resource="http://ontology.ip.rm.cnr.it/ontologies/DOLCE-Lite#Event"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#takesPlaceDuring"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/contact_period"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">interaction between a subject of care and one or more healthcare personnel</rdfs:comment>
        <rdfs:label xml:lang="en">contact</rdfs:label>
    </owl:Class>
    


    <!-- https://contsys.org/concept/contact_period -->

    <owl:Class rdf:about="https://contsys.org/concept/contact_period">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/healthcare_activity_period"/>
        <rdfs:comment xml:lang="en">healthcare activity period during which a contact occurs</rdfs:comment>
        <skos:note xml:lang="en">NOTE 1 Since during a contact, more than one health issue may be addressed, it may relate to more than one healthcare process and more than one episode of care.

NOTE 2 In EN 13940-1:2007 the concept encounter was defined as &apos;contact in the course of which healthcare activities are delivered to a subject of care in her or his presence&apos;.</skos:note>
    </owl:Class>
    


    <!-- https://contsys.org/concept/continuity_facilitator_mandate -->

    <owl:Class rdf:about="https://contsys.org/concept/continuity_facilitator_mandate">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/healthcare_mandate"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#hasTopic"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/health_thread"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#applies"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/care_period_mandate"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">healthcare mandate assigning the right and obligation to monitor and coordinate the delivery of care described in those care period mandates related to healthcare matters linked by specific health threads</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/core_care_plan -->

    <owl:Class rdf:about="https://contsys.org/concept/core_care_plan">
        <rdfs:subClassOf rdf:resource="http://ontology.ip.rm.cnr.it/ontologies/DOLCE-Lite#Mental-Object"/>
        <rdfs:comment xml:lang="en">reusable content and structure for a potential care plan for a specified set of circumstances</rdfs:comment>
        <skos:note xml:lang="en">NOTE 1 A core care plan is usually based upon knowledge in clinical guidelines (including protocols).

NOTE 2 core care plans can be applied in healthcare planning as a clinical process management method.

NOTE 3 A core care plan may include advanced formulated schemes for recommended healthcare activities.</skos:note>
    </owl:Class>
    


    <!-- https://contsys.org/concept/data_repository -->

    <owl:Class rdf:about="https://contsys.org/concept/data_repository">
        <rdfs:subClassOf rdf:resource="http://ontology.ip.rm.cnr.it/ontologies/DOLCE-Lite#Mental-Object"/>
        <rdfs:comment xml:lang="en">an identifiable data storage facility</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/demand_for_care -->

    <owl:Class rdf:about="https://contsys.org/concept/demand_for_care">
        <rdfs:subClassOf rdf:resource="http://ontology.ip.rm.cnr.it/ontologies/DOLCE-Lite#Event"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#justifies"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/demand_mandate"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#asksFor"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/healthcare_provider_activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#concerns"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/healthcare_matter"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#triggers"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/care_period_mandate"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">demand for healthcare provider activities expressed by a healthcare actor</rdfs:comment>
        <rdfs:label xml:lang="en">demandForCare</rdfs:label>
    </owl:Class>
    


    <!-- https://contsys.org/concept/demand_for_initial_contact -->

    <owl:Class rdf:about="https://contsys.org/concept/demand_for_initial_contact">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/demand_for_care"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#resultsIn"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/initial_contact"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">first demand for care concerning one or more specific health issues to be assessed by a healthcare provider</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/demand_mandate -->

    <owl:Class rdf:about="https://contsys.org/concept/demand_mandate">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/healthcare_mandate"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#givesWayTo"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/care_period_mandate"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#permits"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/continuity_facilitator_mandate"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#permits"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/mandate_to_export_personal_information"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#hasTopic"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/healthcare_matter"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">healthcare mandate implying the right and obligation to demand healthcare activities</rdfs:comment>
        <skos:note xml:lang="en">NOTE 1 A demand for care is usually made by a subject of care him/herself, but there are circumstances where the subject of care is not in the position of making a demand for care. In that case, it has to be made on their behalf by another person.

NOTE 2 Within certain jurisdictions a generic demand mandate may be explicitly or implicitly assigned to certain categories of citizens on the basis of their specific roles. It may also be directly or indirectly made necessary by law that such generic mandates are explicitly established.</skos:note>
    </owl:Class>
    


    <!-- https://contsys.org/concept/discharge_report -->

    <owl:Class rdf:about="https://contsys.org/concept/discharge_report">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/clinical_report"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#concerns"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/mandated_period_of_care"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">clinical report concerning a completed, mandated period of care</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/discharge_summary -->

    <owl:Class rdf:about="https://contsys.org/concept/discharge_summary">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/discharge_report"/>
        <rdfs:comment xml:lang="en">discharge report summarizing a mandated period of care</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/dissent -->

    <owl:Class rdf:about="https://contsys.org/concept/dissent">
        <rdfs:subClassOf rdf:resource="http://ontology.ip.rm.cnr.it/ontologies/DOLCE-Lite#Event"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#limits"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/healthcare_mandate"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#requires"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/consent_competence"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">refusal to permit specific healthcare activities to be performed</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/electronic_health_record_component -->

    <owl:Class rdf:about="https://contsys.org/concept/electronic_health_record_component">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/health_record_component"/>
        <rdfs:comment xml:lang="en">health record component which only includes information in electronic format</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/electronic_health_record_extract -->

    <owl:Class rdf:about="https://contsys.org/concept/electronic_health_record_extract">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/health_record_extract"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#isExtractedFrom"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/sharable_data_repository"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#isImportedInto"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/sharable_data_repository"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#aggregationOf"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/electronic_health_record_component"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">health record extract consisting solely of electronic record components</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/electronic_patient_summary -->

    <owl:Class rdf:about="https://contsys.org/concept/electronic_patient_summary">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/electronic_health_record_extract"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#componentOf"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/summarized_healthcare_information_repository"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">electronic health record extract containing essential healthcare information intended for specific uses</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/episode_of_care -->

    <owl:Class rdf:about="https://contsys.org/concept/episode_of_care">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/health_related_period"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#isCentredOn"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/health_issue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">health related period during which healthcare activities are performed to address one health issue as identified by one healthcare professional</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/episodes_of_care_bundle -->

    <owl:Class rdf:about="https://contsys.org/concept/episodes_of_care_bundle">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/health_related_period"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#aggregationOf"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/episode_of_care"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">group of episodes of care delineated by a health thread</rdfs:comment>
        <skos:note xml:lang="en">NOTE 1 An episodes of care bundle starts with the very first contact with a healthcare provider for a health issue considered in a health thread and ends after the completion of all healthcare activities related to the last contact with any healthcare provider for a health issue encompassed in the same health thread.

NOTE 2 As different health threads may be considered that reconcile the perspectives of different healthcare actors (e.g. a care team manager, or a health authority) or sets of healthcare actors, there may exist different health threads according to the specific perspectives of those sets of healthcare actors that justify building up such health threads. As a consequence there may exist as many episodes of care bundles as there are such health threads.

NOTE 3 From the electronic health record point of view, an episodes of care bundle shows the overall healthcare activity period elements related to those health issues that are linked by the same health thread.

NOTE 4 In EN 13940-1:2007 cumulative episode of care was the preferred term for this concept.</skos:note>
    </owl:Class>
    


    <!-- https://contsys.org/concept/excluded_condition -->

    <owl:Class rdf:about="https://contsys.org/concept/excluded_condition">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/considered_condition"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#isNotConsistentWith"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/observed_condition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">considered condition that one or more healthcare professionals have determined not to be consistent with the known observed conditions</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/health_approach -->

    <owl:Class rdf:about="https://contsys.org/concept/health_approach">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/episode_of_care"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#address"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/healthcare_goal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">episode of care during which the healthcare activities performed address one specific healthcare goal</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/health_concern -->

    <owl:Class rdf:about="https://contsys.org/concept/health_concern">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/health_record_extract"/>
        <rdfs:comment xml:lang="en">health record extract that includes all health record components associated with a health thread for a specific concern</rdfs:comment>
        <skos:note xml:lang="en">NOTE 1 A concern is gathered information to support continuity of care for a subject of care. A health thread can include healthcare activities, health conditions, healthcare activity planning, activity management, evaluations and assessments. Thereby, all information needed for continuity of care for an individual subject of care can be covered by a health concern.

NOTE 2 health concerns can be constructed to support continuity of care for healthcare processes, clinical processes and accumulations/associations of clinical processes. This means that concerns can relate to episodes of care, contacts, chronic disease management, multiple chronic disease management, etc.

NOTE 3 health concerns can be represented and labelled in EHR as extracts or components.

NOTE 4 health concern is closely related to the “concern tracking” based on HL7 standards.</skos:note>
    </owl:Class>
    


    <!-- https://contsys.org/concept/health_condition -->

    <owl:Class rdf:about="https://contsys.org/concept/health_condition">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/health_issue"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#governsTheChoiceOf"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/clinical_pathway"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">observed or potential observable aspects of the health state at a given time</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/health_condition_delay -->

    <owl:Class rdf:about="https://contsys.org/concept/health_condition_delay">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/healthcare_activity_delay"/>
        <rdfs:comment xml:lang="en">healthcare activity delay caused by a health condition</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/health_condition_evolution -->

    <owl:Class rdf:about="https://contsys.org/concept/health_condition_evolution">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/health_thread"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#showsTheEvolutionOf"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/health_condition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#relatesTo"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/input_health_state"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">health thread showing the evolution of health conditions during a healthcare process, starting with the health condition that represents the input health state</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/health_condition_period -->

    <owl:Class rdf:about="https://contsys.org/concept/health_condition_period">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/health_related_period"/>
        <rdfs:comment xml:lang="en">health related period during which a health condition has been observed</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/health_issue -->

    <owl:Class rdf:about="https://contsys.org/concept/health_issue">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/healthcare_matter"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#determines"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/healthcare_activity_period_element"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">representation of an issue related to the health of a subject of care as identified by one or more healthcare actors</rdfs:comment>
    </owl:Class>
    


