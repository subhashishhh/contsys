        <rdfs:comment xml:lang="en">healthcare third party having person role</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/output_health_state -->

    <owl:Class rdf:about="https://contsys.org/concept/output_health_state">
