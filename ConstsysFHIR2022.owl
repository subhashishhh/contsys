    </owl:Class>
    


    <!-- https://contsys.org/concept/potential_health_condition -->

    <owl:Class rdf:about="https://contsys.org/concept/potential_health_condition">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/health_condition"/>
