                <owl:onClass rdf:resource="https://contsys.org/concept/healthcare_matter"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#impliesTheRightToCommunicate"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/health_record_extract"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">healthcare mandate implying the right to communicate health record extracts</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/mandated_period_of_care -->

    <owl:Class rdf:about="https://contsys.org/concept/mandated_period_of_care">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/health_related_period"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#isDocumentedIn"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/professional_health_record"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">set of healthcare activity periods where a healthcare provider is mandated to perform the healthcare activities required to address specific health needs. Ex. A hospital stay, a series of radiotherapy sessions at an outpatient clinic.</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/medical_device -->

    <owl:Class rdf:about="https://contsys.org/concept/medical_device">
        <rdfs:subClassOf rdf:resource="http://ontology.ip.rm.cnr.it/ontologies/DOLCE-Lite#Physical-Object"/>
        <rdfs:comment xml:lang="en">any instrument, apparatus, implement, machine, appliance, implant, in vitro reagent or calibrator, software, material or other similar or related article, intended by the manufacturer to be used, alone or in combination, for human beings for one or more of the specific purpose(s) of</rdfs:comment>
        <rdfs:label xml:lang="en">medicalDevice</rdfs:label>
    </owl:Class>
    


    <!-- https://contsys.org/concept/medicinal_product -->

    <owl:Class rdf:about="https://contsys.org/concept/medicinal_product">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/product"/>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.bioontology.org/ontology/SNOMEDCT/763158003</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.hl7.org/fhir/medicinalproduct-definitions.html#MedicinalProduct</oboInOwl:hasDbXref>
        <rdfs:comment xml:lang="en">any substance or combination of substances that can be administered to human beings for treating or preventing disease, with the view to making a medical diagnosis or to restore, correct, or modify physiological functions</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/medium -->

    <owl:Class rdf:about="https://contsys.org/concept/medium">
        <rdfs:subClassOf rdf:resource="http://ontology.ip.rm.cnr.it/ontologies/DOLCE-Lite#Physical-Object"/>
        <rdfs:comment xml:lang="en">medium: material on which data is stored (e.g., a magnetic disk).</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/multi-professional_care_plan -->

    <owl:Class rdf:about="https://contsys.org/concept/multi-professional_care_plan">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/care_plan"/>
        <rdfs:comment xml:lang="en">care plan encompassing healthcare provider activities performed by healthcare professionals having different healthcare professional entitlements</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/needed_healthcare_activity -->

    <owl:Class rdf:about="https://contsys.org/concept/needed_healthcare_activity">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/healthcare_activities_bundle"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#address"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/health_need"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">healthcare activities bundle which includes those healthcare activities assessed as needed to address specified health need</rdfs:comment>
        <skos:note xml:lang="en">NOTE 1 A needed healthcare activity is a healthcare activity that is assessed by healthcare professionals to be motivated/indicated by the health need.

NOTE 2 A needed healthcare activity is the outcome of healthcare needs assessments performed by healthcare professionals. needed healthcare activities can be identified by any mandated healthcare professional performing healthcare needs assessment for a subject of care.

NOTE 3 needed healthcare activities are managed in a care plan.</skos:note>
    </owl:Class>
    


    <!-- https://contsys.org/concept/next_of_kin -->

    <owl:Class rdf:about="https://contsys.org/concept/next_of_kin">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/for-cocd-ontology-781#personRole"/>
        <rdfs:comment xml:lang="en">person role being either the closest living relative of the subject of care or identified as the one he has a close relationship with</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/non-ratified_healthcare_information -->

    <owl:Class rdf:about="https://contsys.org/concept/non-ratified_healthcare_information">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/healthcare_information"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#becomes"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/healthcare_information_for_import"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#isRecordedIn"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/health_record"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">healthcare information the relevance of which has not been assessed and explicitly stated as valid by a healthcare professional</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/observed_condition -->

    <owl:Class rdf:about="https://contsys.org/concept/observed_condition">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/health_condition"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://hl7.org/fhir/Observation.basedOn"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/informed_consent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://hl7.org/fhir/Observation.encounter"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/healthcare_activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://hl7.org/fhir/Observation.performer"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/healthcare_professional"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://hl7.org/fhir/Observation.subject"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/subject_of_care"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#hasBeenObservedDuring"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/health_condition_period"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://hl7.org/fhir/Observation.code"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://hl7.org/fhir/Observation.identifier"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://hl7.org/fhir/Observation.status"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://hl7.org/fhir/Observation.effectiveDateTime"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.bioontology.org/ontology/SNOMEDCT/362976009</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.hl7.org/fhir/observation-definitions.html#Observation</oboInOwl:hasDbXref>
        <rdfs:comment xml:lang="en">health condition observed by a healthcare actor</rdfs:comment>
        <rdfs:seeAlso xml:lang="en">Observation</rdfs:seeAlso>
        <skos:note xml:lang="en">NOTE 1 Healthcare professionals and subjects of care are examples of healthcare actors that can perceive the observed aspect of a health state.
NOTE 2 An observed condition is a health issue and as such is a representation of aspect(s) of the health state.</skos:note>
    </owl:Class>
    


    <!-- https://contsys.org/concept/other_carer -->

    <owl:Class rdf:about="https://contsys.org/concept/other_carer">
