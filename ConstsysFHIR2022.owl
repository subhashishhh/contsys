            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#representsPotentialAspectsOf"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/health_state"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">possible future or current health condition described by a healthcare actor</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/prescribed_self-care -->

    <owl:Class rdf:about="https://contsys.org/concept/prescribed_self-care">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/self-care_activity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#isPrescribedBy"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/healthcare_professional"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#takesPlaceDuring"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/self-care_period"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">self-care activity prescribed by a healthcare professional</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/prescribed_third_party_activity -->

    <owl:Class rdf:about="https://contsys.org/concept/prescribed_third_party_activity">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/healthcare_third_party_activity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#isPrescribedBy"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/healthcare_professional"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://contsys.org/concept/product -->

    <owl:Class rdf:about="https://contsys.org/concept/product">
        <rdfs:subClassOf rdf:resource="http://ontology.ip.rm.cnr.it/ontologies/DOLCE-Lite#Physical-Object"/>
        <rdfs:comment xml:lang="en">output of an organization that can be produced without any transaction taking place between the organization and the customer</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/professional_health_record -->

    <owl:Class rdf:about="https://contsys.org/concept/professional_health_record">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/health_record"/>
        <rdfs:comment xml:lang="en">health record held under the responsibility of one healthcare provider and maintained by one or several healthcare professionals</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/professionally_assessed_condition -->

    <owl:Class rdf:about="https://contsys.org/concept/professionally_assessed_condition">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/observed_condition"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#isIdentifiedBy"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/healthcare_professional"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">observed condition assessed by a healthcare professional concerning the genesis, the course, the severity or the impact of the health state</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/prognostic_condition -->

    <owl:Class rdf:about="https://contsys.org/concept/prognostic_condition">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/potential_health_condition"/>
