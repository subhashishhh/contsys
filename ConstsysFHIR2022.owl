            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#isObservedAs"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/resultant_condition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">health state when a healthcare process ends</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/party -->

    <owl:Class rdf:about="https://contsys.org/concept/party">
        <rdfs:subClassOf rdf:resource="http://www.loa-cnr.it/ontologies/DnS#role"/>
        <rdfs:comment xml:lang="en">person or group performing a role in relation to the business of a specific community or domain</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/personal_health_record -->

    <owl:Class rdf:about="https://contsys.org/concept/personal_health_record">
