            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#isBasedOn"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/professionally_assessed_condition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">possible future or current health condition described by a healthcare actor</rdfs:comment>
    </owl:Class>
    


    <!-- https://contsys.org/concept/protocol -->

    <owl:Class rdf:about="https://contsys.org/concept/protocol">
        <rdfs:subClassOf rdf:resource="https://contsys.org/concept/clinical_guideline"/>
